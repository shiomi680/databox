datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model ShippingModel {
  Id                   Int      @id @default(autoincrement())
  ShipDate             String?
  Title                String?
  ShippingInvoicePrice Float?
  ShippingFileMappings ShippingFileMapping[]
}

model ShippingFileMapping {
  ShippingId Int
  FileId     Int
  ShippingModel   ShippingModel  @relation(fields: [ShippingId], references: [Id])
  FileModel       FileModel      @relation(fields: [FileId], references: [FileId])

  @@id([ShippingId, FileId])
  @@index([ShippingId, FileId], name: "ItemFileMapping_index")
}

model FileModel{
  FileId          Int               @id @default(autoincrement())
  FileName        String?
  FilePath        String?
  UploadTimestamp DateTime @default(now())
  ShippingFileMappings ShippingFileMapping[]
}

generator client {
  provider = "prisma-client-js"
}